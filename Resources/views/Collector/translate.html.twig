{% extends 'WebProfilerBundle:Profiler:layout.html.twig' %}

{% block toolbar %}
    {% set counter = 0 %}

    <div class="sf-toolbar-block">
        <div class="sf-toolbar-info">
            {% for name,data in collector.data %}
                {% set counter = counter + data|length %}

                <div class="sf-toolbar-info-piece">
                    <b>{{ name }} calls</b>
                    <span>{{ data|length }}</span>
                </div>
            {% endfor %}

            <div class="sf-toolbar-info-piece">
                <b>Total calls</b>
                <span class="sf-toolbar-status sf-toolbar-status-green">{{ counter }}</span>
            </div>
        </div>
        <div class="sf-toolbar-icon">
            <a href="{{ path('_profiler', { 'token': token, 'panel': name }) }}">
                <span class="icon"><img src="data:image/png;base64," alt="" /></span>
                <span class="sf-toolbar-status">{{ counter }}</span>
            </a>
        </div>
    </div>
{% endblock %}

{% block menu %}
    {% set counter = 0 %}

    {% for name,data in collector.data %}
        {% set counter = counter + data|length %}
    {% endfor %}
    <span class="label">
        <span class="icon"><img src="data:image/png;base64," alt="" /></span>
        <strong>Translations</strong>
        <span class="count">
            <span>{{ counter }}</span>
        </span>
    </span>
{% endblock %}

{% block panel %}
    <h2>Google Translate API calls detail</h2>

    {% for name, calls in collector.data %}
        <h3>{{ name }}</h3>

        {% if calls|length == 0 %}
            <p><em>No calls profiled for this method.</em></p>
        {% else %}
            <table>
                <tr>
                    <th>#</th>
                    <th>Query</th>
                    {% if calls[1].source is not empty %}
                        <th>Source</th>
                    {% endif %}
                    {% if calls[1].target is not empty %}
                        <th>Target</th>
                    {% endif %}
                    <th>Duration</th>
                    <th>Memory usage</th>
                </tr>

                {% for call in calls %}
                    <tr>
                        <td>{{ loop.index }}</td>
                        <td>{{ call.query }}</td>
                        {% if call.source is not empty %}
                            <td>{{ call.source }}</td>
                        {% endif %}
                        {% if call.target is not empty %}
                            <td>{{ call.target }}</td>
                        {% endif %}
                        <td>{{ call.duration }} ms</td>
                        <td>{{ call.memory_end }}</td>
                    </tr>
                {% endfor %}
            </table>
        {% endif %}
    {% endfor %}
{% endblock %}